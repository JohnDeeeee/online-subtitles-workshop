<div class="row">
	<div class="panel panel-primary col-md-13">
	  <!-- Default panel contents -->
	  <div class="panel-heading">{title}</div>
	  <!-- Table -->
	  <table class="table table-striped">
	  	<tbody>
	  	<tr><td rowspan="15" class="col-md-4 text-center"> <img style="width:245px; height:245px;" src="{TemplatePath}/tmp/icon-user-default.png" /> </td></tr>
		<tr><td colspan="2"><b>General Information:</b></td></tr>
		<tr><td>Member Since:</td><td>{JoinDate}</td></tr>
		<tr><td>Last Login:</td><td>{LastLogin}</td></tr>
		<tr><td>Nick Name:</td><td>{NickName}</td></tr>
		<tr><td>Gender:</td><td>{Gender}</td></tr>
		<tr><td>Date Of Birth:</td><td>{DateOfBirth}</td></tr>
		<tr><td>Joined Teams:</td><td>{JoinedTeams}</td></tr>
		<tr><td>Created Teams:</td><td>{CreatedTeams}</td></tr>
		<tr><td>Own Subtitles:</td><td>{OwnSubtitles}</td></tr>
		<tr><td colspan="2"><b>contact information:</b></td></tr>
		<tr><td>Email:</td><td>{email}</td></tr>
		<tr><td>Websit:</td><td>{website}</td></tr>
		<tr><td>Phone:</td><td>{phone}</td></tr>
		<tr><td>Skype:</td><td>{skype}</td></tr>
	  	</tbody>
	  </table>
	</div>
</div>
